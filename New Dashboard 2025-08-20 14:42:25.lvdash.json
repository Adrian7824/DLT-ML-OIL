{
  "datasets": [
    {
      "name": "4357dd46",
      "displayName": "Producción mensual por pozo",
      "queryLines": [
        "SELECT pozo_id, mes, oil_bbl\r\n",
        "FROM oil.dev.gold_monthly_kpis_tbl\r\n",
        "ORDER BY pozo_id, mes;"
      ]
    },
    {
      "name": "eec215d2",
      "displayName": "Lifting cost promedio por mes",
      "queryLines": [
        "SELECT mes, AVG(lifting_cost_usd_bbl) AS avg_lifting_cost\r\n",
        "FROM oil.dev.gold_monthly_kpis_tbl\r\n",
        "GROUP BY mes\r\n",
        "ORDER BY mes;\r\n"
      ]
    },
    {
      "name": "c3663a78",
      "displayName": "Margen mensual por pozo",
      "queryLines": [
        "SELECT\r\n",
        "  mes,\r\n",
        "  pozo_id,\r\n",
        "  margen_usd\r\n",
        "FROM oil.dev.gold_monthly_kpis_tbl\r\n",
        "ORDER BY mes, pozo_id;\r\n"
      ]
    },
    {
      "name": "1000f576",
      "displayName": "Real vs prediction",
      "queryLines": [
        "SELECT\r\n",
        "  p.pozo_id,\r\n",
        "  p.mes,\r\n",
        "  k.oil_bbl       AS real_oil_bbl,\r\n",
        "  p.y_hat_oil_bbl_next AS pred_oil_bbl_next,\r\n",
        "  (p.y_hat_oil_bbl_next - k.oil_bbl) AS error\r\n",
        "FROM oil.dev.gold_predictions p\r\n",
        "LEFT JOIN oil.dev.gold_monthly_kpis_tbl k\r\n",
        "  ON p.pozo_id = k.pozo_id AND p.mes = k.mes\r\n",
        "ORDER BY p.pozo_id, p.mes;"
      ]
    }
  ],
